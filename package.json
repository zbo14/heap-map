{
  "name": "heap-map",
  "version": "0.0.1",
  "description": "A binary heap with a mapping of elements to indexes for more efficient lookups and modifications.",
  "main": "index.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "lint": "standard",
