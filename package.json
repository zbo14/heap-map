  "keywords": [
    "binary heap",
    "data structures",
    "heap",
    "map",
    "mapping",
    "priority queue",
    "queue"
  ],
  "author": "Zachary Balder",
  "license": "MIT",
  "devDependencies": {
    "mocha": "^9.1.2",
    "nyc": "^15.1.0",
    "standard": "^16.0.3"
  },
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "reporter": [
      "html",
      "text"
    ]
  }
}
